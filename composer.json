{
    "name": "colorcube/cookie-redirect",
    "type": "typo3-cms-extension",
    "description": "One-Time Forwarder TYPO3 Extension",
    "keywords": [
        "TYPO3",
        "extension",
        "redirect"
    ],
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/colorcube/cookie_redirect",
    "support": {
        "issues": "https://github.com/colorcube/cookie_redirect/issues"
    },
    "authors": [
        {
            "name": "RenÃ© Fritz",
            "email": "r.fritz@colorcube.de",
            "role": "Developer",
            "homepage": "http://www.colorcube.de"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "typo3/cms-core": "^7.6 || ^8.7"
    },
    "autoload": {
        "psr-4": {
            "Colorcube\\CookieRedirect\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/cookie-redirect": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "cookie_redirect"
        }
    }
}
